{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "arcanedev/log-viewer": "^5.2",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "highideas/laravel-users-online": "^3.0",
        "imanghafoori/laravel-masterpass": "^2.0",
        "intervention/image": "^2.7",
        "joisarjignesh/bigbluebutton": "^1.1",
        "laravel/framework": "6.*",
        "laravel/helpers": "^1.1",
        "laravel/tinker": "^1.0",
        "laravelium/sitemap": "^6.0",
        "livewire/livewire": "^1.1",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.2",
        "nwidart/laravel-modules": "^5.0",
        "pbmedia/laravel-ffmpeg": "^7.4",
        "php-ffmpeg/php-ffmpeg": "^0.16.0",
        "phpoffice/phpword": "^0.17.0",
        "pion/laravel-chunk-upload": "^1.4",
        "predis/predis": "^1.1",
        "shetabit/visitor": "^2.1",
        "simplesoftwareio/simple-qrcode": "~3",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/microsoft": "^4.1",
        "socialiteproviders/microsoft-azure": "^4.2",
        "spatie/laravel-analytics": "^3.10",
        "spatie/laravel-permission": "^3.16",
        "tormjens/eventy": "^0.7.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "beyondcode/laravel-dump-server": "^1.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "jzonta/faker-restaurant": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.2.20"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
